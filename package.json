{
  "name": "take-latest-generator-co",
  "private": true,
  "homepage": "https://github.com/maotong06/take-latest-generator-co",
  "description": "利用generator解决竞态情况，在少量修改原代码的情况下，就能解决",
  "version": "0.2.0",
  "type": "module",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./dist": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./dist/*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*": "./*"
  },
  "license": "MIT",
  "author": "limaotong",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maotong06/take-latest-generator-co.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:vue": "vite serve dev/vue/ --config vite.config.js -- --devVue",
    "dev:react": "vite serve dev/react/ --config vite.config.js -- --devReact",
    "build": "tsc && vite build && tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "axios": "^1.6.4",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-mock-data": "^4.0.5",
    "vue": "^3.3.11"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
